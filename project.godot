Focus="*res://scripts/managers/input_focus_manager.gd"
UiSoundManager="*res://scenes/managers/ui_sound_manager.tscn"

[display]

window/size/mode=3
window/size/window_width_override=1728
window/size/window_height_override=972
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_state_charts/plugin.cfg")

