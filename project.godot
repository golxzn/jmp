run/max_fps=60
boot_splash/bg_color=Color(0.298039, 0.298039, 0.298039, 1)
config/icon="res://assets/images/textures/cubes/cube.png"

[audio]

bus_volume_db/volume/music=-6.0206
bus_volume_db/volume/player_sfx=-1.938
bus_volume_db/volume/environment_sfx=-1.938
bus_volume_db/volume/UI=-6.021

[autoload]

